                              <Insets />
                           </opaqueInsets>
                        </JFXButton>
                        <JFXButton fx:id="btn_Pedidos" alignment="CENTER_RIGHT" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" ripplerFill="WHITE" stylesheets="@../resources/css/White_Botoes.css" text="Pedidos">
