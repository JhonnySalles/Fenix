<AnchorPane prefHeight="600.0" prefWidth="700.0" stylesheets="@../resources/css/White_Dashboard.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Administrador.gui.DashboardController">
